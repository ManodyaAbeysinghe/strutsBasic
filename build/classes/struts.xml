<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE struts PUBLIC 
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" 
"http://struts.apache.org/dtds/struts-2.0.dtd">  

<struts>
<constant name ="struts.devMode" value="true"/>
<package name="default" extends="struts-default">

<action name ="log" class="com.im.struts.actions.AuthAction">
<result name="success">/success.jsp</result>
<result name="fail">/fail.jsp</result>
<result name="error">/error.jsp</result>
</action>

<action name ="reg" class="com.im.struts.actions.RegisterAction" method="register">
<result name="success">/regsuccess.jsp</result>
<result name="fail">/regfail.jsp</result>
<result name="error">/error.jsp</result>
</action>



</package>

</struts>
